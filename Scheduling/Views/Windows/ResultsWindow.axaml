<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Scheduling.Views"
        xmlns:scheduling="clr-namespace:Scheduling"
        xmlns:viewModels="clr-namespace:Scheduling.ViewModels"
        xmlns:userControls="clr-namespace:Scheduling.Views.UserControls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Scheduling.Views.Windows.ResultsWindow"
        Title="ResultsWindow">
    
    <Design.DataContext>
        <viewModels:ResultsViewModel/>
    </Design.DataContext>
    
    <userControls:ScheduleUserControl DataContext="{Binding ScheduleViewModel}"/>
    
</Window>
